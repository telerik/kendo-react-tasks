{
  "name": "@telerik/kendo-react-tasks",
  "description": "An internal utility package exporting gulp tasks for the Kendo React components infrastructure",
  "version": "0.0.0-semantically-released",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-react-tasks.git"
  },
  "main": "gulp-tasks.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post"
  },
  "keywords": [
    "Kendo UI",
    "React"
  ],
  "dependencies": {
    "@telerik/eslint-config": "1.1.0",
    "@telerik/kendo-common-tasks": "^1.8.0",
    "babel-core": "~6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-umd": "~6",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "~6",
    "babel-preset-react": "~6",
    "babel-preset-stage-1": "~6",
    "babel-traverse": "6.3.19",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "enzyme": "^2.0.0-rc1",
    "extract-text-webpack-plugin": "^0.9.1",
    "gulp-jasmine": "^2.2.1",
    "jasmine-reporters": "^2.1.1",
    "jasmine-spec-reporter": "^2.4.0",
    "jquery": "^2.2.1",
    "json-loader": "^0.5.4",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "source-map-loader": "^0.1.5"
  },
  "author": "Telerik",
  "license": "Apache-2.0",
  "devDependencies": {
    "@telerik/semantic-prerelease": "^1.1.5",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    }
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  }
}
